{
  "id": "657654c6-ae43-45cf-94c9-f0488f0006ee",
  "name": "RegisterDeployment",
  "friendlyName": "Register Deployment with Raygun",
  "description": "Register Deployment with Raygun",
  "author": "Gareth van der Berg",
  "helpMarkDown": "[More Information](https://marketplace.visualstudio.com/items?itemName=raygun.vsts-extension)",
  "category": "Deploy",
  "visibility": [
    "Build",
    "Release"
  ],
  "demands": [],
  "version": {
    "Major": "1",
    "Minor": "0",
    "Patch": "0"
  },
  "instanceNameFormat": "Register $(version) with Raygun",
  "inputs": [
    {
      "name": "authType",
      "type": "pickList",
      "label": "Authentication Method",
      "defaultValue": "ServiceEndpoint",
      "helpMarkDown": "",
      "options": {
        "ServiceEndpoint": "Service Endpoint",
        "ExternalAccessToken": "External Access Token"
      }
    },
    {
      "name": "serviceEndpoint",
      "type": "connectedService:raygun",
      "label": "Service Endpoint",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": "The TFS/Team Services service endpoint that specifies the identity used to upload the symbols.",
      "visibleRule": "authType = ServiceEndpoint"
    },
    {
      "name": "externalAccessToken",
      "type": "string",
      "label": "External Access Token",
      "defaultValue": "",
      "isConfidential": true,
      "required": true,
      "helpMarkDown": "You can create this token within your user settings in Raygun, just navigate to (https://app.raygun.io/user) and check the External Access Token section.",
      "visibleRule": "authType = ExternalAccessToken"
    },
    {
      "name": "apiKey",
      "type": "string",
      "label": "Api Key",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": ""
    },
    {
      "name": "version",
      "type": "string",
      "label": "Version",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": ""
    },
    {
      "name": "ownerName",
      "type": "string",
      "label": "Owner Name",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": ""
    },
    {
      "name": "emailAddress",
      "type": "string",
      "label": "Email Address",
      "defaultValue": "",
      "required": true,
      "helpMarkDown": ""
    },
    {
      "name": "scmIdentifier",
      "type": "string",
      "label": "Scm Identifier",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": ""
    },
    {
      "name": "releaseNotes",
      "type": "string",
      "label": "Release Notes",
      "defaultValue": "",
      "required": false,
      "helpMarkDown": ""
    }
  ],
  "execution": {
    "Node": {
      "target": "register-deployment.js"
    },
    "PowerShell3": {
      "target": "RaygunDeploy.ps1"
    }
  },
  "messages": {
    "SuccessfullyRegistered": "Successfully registered deployment with Raygun",
    "UploadingSymbols": "Registering deployment",
    "ServerErrorGeneric": "Registering deployment failed.",
    "ServerErrorUnauthorized": "Registering deployment failed with error 'Unauthorized'. Please check the credentials used.",
    "ServerErrorNotFound": "Registering deployment failed with error 'Not Found'. Please make sure the Raygun application '%s' exists."
  }
}